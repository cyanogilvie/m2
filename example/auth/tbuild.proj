application "examplecomponent" {
	version 0.1

	requires {
		m2
		netdgram::tcp
		sop
		cflib
		dsl
		crypto

		sha1
	}

	files {
		component.tcl
	}

	platform tcl
}

application "exampleclient" {
	version 0.1

	requires {
		m2
		netdgram::tcp
		sop
		cflib
		dsl
		crypto

		sha1
	}

	files {
		client.tcl
	}

	platform tcl
}

# vim: ft=tcl foldmethod=marker foldmarker=<<<,>>> ts=4 shiftwidth=4
